{
  "manifest_version": 3,
  "name": "Queue Movie Exporter",
  "version": "1.1",
  "description": "Extrahiert Filme und Serien aus Queue, kategorisiert diese und speichert sie formatiert in einer xlsx Datei.",
  "permissions": ["activeTab", "scripting"],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  }
}