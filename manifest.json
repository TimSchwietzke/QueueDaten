{
  "manifest_version": 3,
  "name": "Queue Movie Exporter",
  "version": "1.2.1",
  "description": "Extrahiert Filme und Serien aus Queue, kategorisiert diese und speichert sie formatiert in einer xlsx Datei.",
  "permissions": ["activeTab", "scripting"],
  "icons": {
    "16": "src/icons/icon16.png",
    "48": "src/icons/icon48.png",
    "128": "src/icons/icon128.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "src/icons/icon16.png",
      "48": "src/icons/icon48.png",
      "128": "src/icons/icon128.png"
    }
  },
  "web_accessible_resources": [
    {
      "resources": ["src/cursor/*", "src/icons/*"],
      "matches": ["<all_urls>"]
    }
  ]
}